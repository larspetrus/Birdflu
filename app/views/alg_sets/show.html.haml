.algset_top
  =link_to('Birdflu', '/')
  =link_to('Algsets', alg_sets_path)

%h3 Show

%table.algsets
  %tr
    %td id
    %td
      =@algset.id
  %tr
    %td name
    %td
      =@algset.name
  %tr
    %td description
    %td
      =@algset.description
  %tr
    %td subset
    %td
      =@algset.subset
  %tr
    %td algs
    %td.small
      =@algset.algs
  %tr
    %td coverage
    %td
      = "#{@algset.coverage}/#{@algset.subset_pos_ids.count - 1}"
  %tr
    %td
    %td
      =link_to('edit', edit_alg_set_path(@algset), class: 'show-pig')
